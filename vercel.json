{
    "version": 2,
    "name": "ecommerce",
    "builds": [
        {
            "src": "./backend/server.js",
            "use": "@vercel/node"
        },
        {
            "src": "./frontend/build",
            "use": "@vercel/static"
        }
    ],
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/backend/server.js"
        }
    ]
}